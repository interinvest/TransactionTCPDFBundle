{
  "name": "interinvest/transaction-tcpdf-bundle",
  "type": "symfony-bundle",
  "description": "A bundle to make TCPDF transaction into Symfony2",
  "keywords": ["pdf", "tcpdf", "transaction", "FPDI"],
  "homepage": "https://github.com/interinvest/TransactionTCPDFBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Joyd Sousa",
      "email": "jsousa@inter-invest.fr"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": ">=2.0",
    "tecnickcom/tcpdf": "6.2.*",
    "setasign/fpdi": "1.6.*"
  },
  "conflict": {
    "setasign/fpdf": "1.7.*"
  },
  "suggest": {
    "setasign/fpdi-tcpdf": "FPDI is required for the us of this bundle"
  },
  "autoload": {
    "psr-0": { "InterInvest\\TransactionTCPDFBundle": "" }
  },
  "target-dir": "InterInvest/TransactionTCPDFBundle"
}